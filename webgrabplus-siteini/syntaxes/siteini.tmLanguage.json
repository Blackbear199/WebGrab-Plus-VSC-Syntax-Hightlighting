					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"},
						"2": {"name": "entity.name.type.siteini"},
						"3": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "(\\|)(primary|mdb_show_id|mdb_episode_id|p[1-9])(\\|)",
					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"},
						"2": {"name": "entity.name.type.siteini"},
						"3": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "\\s*(and|or|xor|shiftl|shiftr|not)\\}",
					"captures":
