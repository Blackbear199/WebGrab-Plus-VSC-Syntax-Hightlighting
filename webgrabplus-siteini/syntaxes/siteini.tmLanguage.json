					{
						"1": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "^\\s*(if|loop)\\s+\\{\\(",
					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "^\\s*(else)\\s+",
