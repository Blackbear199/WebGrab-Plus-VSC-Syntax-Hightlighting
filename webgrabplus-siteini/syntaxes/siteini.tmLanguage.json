					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "[\\{\\|](allowlastdayoverflow|keepindexpage|nopageoverlaps|keeptabs)",
					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "\\.(headers)\\s*",
					"captures":
					{
						"1": {"name": "entity.name.type.siteini"}
					}
				},
				{
