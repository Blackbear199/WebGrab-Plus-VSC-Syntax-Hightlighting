						"1": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "\\.(modify)\\s*\\{\\s*(sort)\\(\\s*(ascending|descending)\\s*,\\s*(string|integer|datetime)\\s*\\)",
					"captures":
					{
						"1": {"name": "entity.name.type.siteini"},
						"2": {"name": "entity.name.tag.siteini"},
						"3": {"name": "entity.name.function.siteini"},
						"4": {"name": "entity.name.function.siteini"}
					}
				},
				{
					"match": "\\((sort)@([0-9]{2}:[0-9]{2}),([0-9]{2}:[0-9]{2}),?([0-9]{2}:[0-9]{2})?,?([0-9]{2}:[0-9]{2})?,?([0-9]{2}:[0-9]{2})?,?([0-9]{2}:[0-9]{2})?\\)",
					"captures":
					{
						"1": {"name": "entity.name.tag.siteini"},
						"2": {"name": "entity.name.function.siteini"},
						"3": {"name": "entity.name.type.siteini"},
						"4": {"name": "entity.name.function.siteini"},
						"5": {"name": "entity.name.type.siteini"},
						"6": {"name": "entity.name.function.siteini"},
