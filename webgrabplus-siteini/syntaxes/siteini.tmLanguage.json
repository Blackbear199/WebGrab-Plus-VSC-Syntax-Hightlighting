					{
						"1": {"name": "entity.name.tag.siteini"},
						"2": {"name": "entity.name.function.siteini"},
						"3": {"name": "entity.name.tag.siteini"},
						"4": {"name": "entity.name.type.siteini"},
						"5": {"name": "entity.name.tag.siteini"},
						"6": {"name": "entity.name.type.siteini"},
						"7": {"name": "entity.name.tag.siteini"}
					}
				},
				{
					"match": "[\\{\\|](customheader|accept|referer|contenttype|postdata|host|credentials|allowautoredirect|expect|list)",
					"captures":
